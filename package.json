{
  "name": "intentguard",
  "version": "1.7.1",
  "description": "Measure Trust Debt in your Git repo - the gap between your docs and code. Like test coverage for documentation accuracy. Run: npx intentguard",
  "keywords": [
    "trust-debt",
    "technical-debt",
    "code-quality",
    "alignment",
    "drift-detection",
    "intent-reality",
    "git-hooks",
    "ci-cd",
    "developer-tools",
    "code-analysis",
    "documentation-drift",
    "project-alignment"
  ],
  "homepage": "https://github.com/wiber/IntentGuard",
  "bugs": {
    "url": "https://github.com/wiber/IntentGuard/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wiber/IntentGuard.git"
  },
  "license": "MIT",
  "author": "Elias Moosman <elias@wiber.dev>",
  "main": "lib/index.js",
  "bin": {
    "intentguard": "./bin/cli.js"
  },
  "files": [
    "bin",
    "lib",
    "src",
    "templates"
  ],
  "scripts": {
    "test": "jest",
    "lint": "eslint src test",
    "format": "prettier --write \"src/**/*.js\" \"test/**/*.js\"",
    "analyze": "node bin/cli.js analyze",
    "report": "node bin/cli.js report",
    "badges": "node generate-badges.js",
    "badges:update": "node generate-badges.js --update",
    "analyze:self": "npm run analyze && npm run badges:update"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "chart.js": "^4.2.1",
    "commander": "^9.4.1",
    "cosine-similarity": "^1.0.1",
    "glob": "^8.0.3",
    "inquirer": "^8.2.5",
    "ora": "^5.4.1",
    "simple-git": "^3.15.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "eslint": "^8.29.0",
    "jest": "^29.3.1",
    "prettier": "^2.8.1"
  },
  "peerDependencies": {
    "husky": "^8.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
